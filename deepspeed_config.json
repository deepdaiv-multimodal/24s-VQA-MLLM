{
  "train_batch_size": 8,
  "gradient_accumulation_steps": 1,
  "steps_per_print": 1000,
  "optimizer": {
    "type": "AdamW",
    "params": {
      "lr": 5e-05,
      "betas": [0.9, 0.999],
      "weight_decay": 0.01
    }
  },
  "fp16": {
    "enabled": true,
    "loss_scale": 0,
    "initial_scale_power": 32,
    "loss_scale_window": 1000,
    "hysteresis": 2,
    "min_loss_scale": 1
  },
  "logging_level": "ERROR"
}
